[
  {
    "type": "terrarium:transform_heights",
    "height_component": "terrarium:height",
    "height_scale": {
      "terrarium:mul": {
        "left": "@height_scale",
        "right": "@world_scale"
      }
    },
    "height_offset": "@height_origin"
  },
  {
    "type": "terrarium:height_noise",
    "height_component": "terrarium:height",
    "octaves": 2,
    "noise_xz_scale": 0.08,
    "noise_y_scale": "@noise_scale"
  },
  {
    "type": "terrarium:osm_coastlines",
    "height_component": "terrarium:height",
    "cover_component": "terrarium:cover",
    "osm_component": "terrarium:osm",
    "lat_lng_coordinate": "earth_lat_lng"
  },
  {
    "type": "terrarium:beaches",
    "cover_component": "terrarium:cover",
    "beach_size": "@beach_size"
  },
  {
    "type": "terrarium:water_edge_flatten",
    "height_component": "terrarium:height",
    "cover_component": "terrarium:cover",
    "flatten_range": 16
  }
]
